<!--pages/API/choice-person/choice-person.wxml-->
<view class="container">
     <view class="main-box">


          <checkbox-group class='free-radios' bindchange=" bindCheckbox">
               <view class="address-ul" wx:for="{{items}}" wx:key="{{items}}" wx:for-index="idx">
                    <view class="ul-top">
                         <image class="check" src='{{item.url}}'></image>
                         <view class="top-name">{{item.name}}</view>
                         <view class="top-tell">(成人)</view>
                         <image class="xiugai" src="http://image.365zhiding.com/wxapp/20171121/xiugai.png"></image>
                    </view>
                    <view class="ul-bottom">身份证：
                         <view class="idcal">{{item.ID}}</view>
                    </view>
                    <view class="ul-bottom">联系电话：
                         <view class="call">{{item.call}}</view>
                    </view>
               </view>
          </checkbox-group>


