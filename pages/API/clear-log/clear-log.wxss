/* pages/API/clear-log/clear-log.wxss */
.container {
  height: 100vh;
 box-sizing: border-box;
  margin: 0;
  padding: 0;
  background: #f7f7f7;
  font-family:PingFang-SC-Regular;
}
.clear_log{
  width: 100%;
  background:#FFFFFF ;
  border-top: 1rpx solid #EEEEEE  ;
  box-sizing: border-box;
  
}

.money_sec{
  width: 100%;
  line-height: 99rpx;
  padding: 0rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  box-sizing: border-box;
  font-family:PingFang-SC-Regular;
  color:rgba(51,51,51,1) ;
  border-bottom: 1rpx solid #EEEEEE  ;
  
}
/* 选项卡开始 */
.clear_open{
  width:100%;
  margin-bottom: 10rpx;
  padding: 0 30rpx 0 60rpx;
   box-sizing: border-box;

}
.open{
  width: 100%;
  line-height: 99rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  box-sizing: border-box;
  font-family:PingFang-SC-Regular;
  color:rgba(153,153,153,1)  ;
  border-bottom: 1rpx solid #EEEEEE;
}
.data{
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  color:rgba(102,102,102,1);

}
.data_dif{
  color: rgba(255,102,0,1) ;
}
.detail{
  border-bottom: 10rpx solid #F7F7F7;
}
.hide{
 display:none;
}
.show{ 
 display: block;
}
/* 选项卡结束 */
.price{
  color: #666666 ;
}