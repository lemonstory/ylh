/* pages/API/my-order-detail/my-order-detail.wxss */
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  background: #f7f7f7;
}
/*出行时间开始  */
.go {
  width: 100%;
  height: auto;
  background: #fff;
  margin-top: 5rpx;
}
.go-time {
  width: 100%;
  height: 84rpx;
  font-size: 32rpx;
  font-family: PingFang-SC-Regular;
  color:#333;
  padding-left:30rpx;
  padding-right: 30rpx;
  display: flex;
  border-bottom:1rpx solid #eee;
  align-items: center;
}
.go-for{
  width:46rpx;
  height:46rpx;
  margin-left: 550rpx;
}
.go-for image{
  width:100%;
  height:100%;
}
/*酒店信息结束  */

/*联系人信息开始  */
.lianxi-box{
  width:100%;
  height:auto;
  margin-top: 10rpx;
  background: #fff;
}
.lianxi-message{
  width: 690rpx;
  height:auto;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-bottom: 1rpx solid #eee;
 font-size:28rpx;
 font-family:PingFang-SC-Regular;
 color:rgba(51,51,51,1);
 line-height: 84rpx;
}
.input-name{
  width: 630rpx;
  height:auto;
  margin-left: 60rpx;
  margin-right: 60rpx;
  border-bottom: 1rpx solid #eee;
  font-size:28rpx;
 font-family:PingFang-SC-Regular;
 color:rgba(51,51,51,1);
 line-height: 84rpx;
 display: flex;
}
.input-shui{
  border-bottom:none;
}


/*联系人信息结束  */

