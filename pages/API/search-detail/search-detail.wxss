.container {
  height: 100vh;
  display: flex;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  background: #f7f7f7;
}
.main{
  margin-top: 5rpx;
  background: #fff;
  width: 100%;
  height: 1334rpx;
}
.search{
  display: flex;
  align-items: center;
  padding-bottom: 10rpx;
  border-bottom:1rpx solid #eee;
}
#myInput {
  width: 580rpx;
  height: 68rpx;
  background:#E5E5E5;
  border-radius: 10rpx;
  display: flex;
  align-content: center;
  box-sizing: border-box;
  margin-left: 20rpx;
  margin-top: 10rpx;
  position: relative;
  font-size:24rpx;
font-family:PingFang-SC-Regular;
color:rgba(51,51,51,1);
}
.cancel{
  width: 120rpx;
