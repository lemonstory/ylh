/* pages/API/store-setting/store-setting.wxss */
.container{
  height: 100vh;
  display: flex;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  background: #f7f7f7;
}
.main{
  width: 100%;
  height:auto;
  margin-top: 5rpx;
}
.logo{
  width: 100%;
  height:auto;
  padding: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  font-size:26rpx;
  font-family:PingFang-SC-Regular;
  color:rgba(51,51,51,1);
  background: #fff;
  border-bottom: 1rpx solid #eee;
}
.logo-img{
  width: 62rpx;
  height:62rpx;
}
.back{
  width:14rpx;
  height:24rpx;
  margin-left:30rpx; 
}
.logo-right{
  display: flex;
  align-items: center;
}