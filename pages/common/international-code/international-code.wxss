/* pages/API/switchcity/switchcity.wxss */

.searchLetter {
  position: fixed;
  right: 0;
  width: 80rpx;
  height: 100%;
  text-align: center;
  justify-content: center;
  display: flex;
  flex-direction: column;
  color: #666;
  z-index: 1;
  top:0;
  bottom:0;
  margin: auto;
  align-items: flex-end;
 padding-right: 10rpx;
}
.touchClass {
  color: #f60;
  font-size: 22rpx;
  }
 
.item_letter {
  display: flex;
  color: #f60;
  height: 60rpx;
  padding-left: 30rpx;
  align-items: center;
} 

.item_city {
  width: 630rpx;
  display: flex;
  height: 88rpx;
  margin-left: 60rpx;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  color: rgba(51, 51, 51, 1);
  border-bottom: 1px solid #eee;
 }
 .choicecity{
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  color: #000;
  padding-left: 60rpx;
 }
