/* pages/API/logo/logo.wxss */
.container{
  height: 100vh;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  border-top:5rpx solid #f7f7f7;
  background: #fff;
  font-family:PingFang-SC-Regular;
}
.logo-img{
width:750rpx;
height:700rpx; 
background: orangered;
position: absolute;
top:160rpx;
}
.logo-img image{
  width: 100%;
  height:100%;
}
.bottom{
width:690rpx;
height:88rpx; 
background:rgba(247,130,16,1);
border-radius: 10rpx;
font-size:32rpx;
font-family:PingFang-SC-Regular;
color:rgba(255,255,255,1);
position: absolute;
bottom: 30rpx;
left:30rpx;
line-height: 88rpx;
text-align: center;
}