/* pages/API/search/search.wxss */
.container {
  height: 100vh;
  display: flex;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  background: #f7f7f7;
}
.main{
  margin-top: 5rpx;
  background: #fff;
  width: 100%;
  height: 1334rpx;
}
.search{
  display: flex;
  align-items: center;
  position: relative;
}
#keyword {
  width: 580rpx;
  height: 68rpx;
  background:#E5E5E5;
  border-radius: 10rpx;
  display: flex;
  align-content: center;
  box-sizing: border-box;
  margin-left: 20rpx;
  margin-top: 10rpx;
  position: relative;
}

.search-icon{
  height:32rpx;
  width:32rpx;
  position: absolute;
  top:32rpx;
  left:40rpx;
}

.cancel{
  width: 120rpx;
  height:68rpx;
  border: 1rpx solid #eee;
  margin-left: 10rpx;
  font-size:28rpx;
 font-family:PingFang-SC-Regular;
 color:rgba(102,102,102,1);
 display: flex;
 align-items: center;
 margin-top: 10rpx;
 justify-content: center;
 border-radius: 10rpx;
}
.route-hot{
width:228rpx;
height:30rpx; 
font-size:32rpx;
font-family:PingFang-SC-Regular;
color:rgba(51,51,51,1);
margin-top: 60rpx;
margin-left: 30rpx;
}
.add-url{
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  width:690rpx;        
  height:auto; 
  /* background: red; */
  display: flex;
  flex-wrap: wrap;
}
.add-li{
  width: auto;
  height:58rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  line-height:58rpx;
  box-sizing: border-box; 
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  color:rgba(102,102,102,1);
  margin-left:10rpx;
  border: 1rpx solid #eee; 
  border-radius: 10rpx;
  margin-bottom: 10rpx;
}


.tuijuan{
  width: 690rpx;
  height:98rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: space-between;
  font-size:28rpx;
 font-family:PingFang-SC-Regular;
 color:rgba(51,51,51,1);
 align-items: center;
}
.more{
  display: flex;
  color:rgba(153,153,153,1);
}
.back{
  width: 14rpx;
  height:24rpx;
  margin-left: 10rpx;
  margin-top: 8rpx;
}
.tui-box{
  width: 690rpx;
  height:auto;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: space-between;
  font-size:28rpx;
 font-family:PingFang-SC-Regular;
 color:rgba(51,51,51,1);
 padding-bottom: 30rpx;
 padding-top: 30rpx;
 align-items: center;
}
.tui-text{
width:510rpx;
height:auto; 
font-size:28rpx;
font-family:PingFang-SC-Regular;
color:rgba(51,51,51,1);
}
.tui-money{
  color:#FF6600;
  font-size:30rpx;
  display: flex;
  align-items: center;
}
.qi{
  color:#999;
  font-size:24rpx;
}
.tui-text1{
display: flex;
flex-direction: column;
}

.close{
  width:36rpx;
  height:36rpx;
  position: absolute;
  right:180rpx;
  top:30rpx;
  z-index: 999;
}
.close image{
width:100%;
height:100%;
}
