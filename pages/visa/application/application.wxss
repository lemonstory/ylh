/* pages/visa/application/application.wxss */
.container{
width: 750rpx;
height:100vh;  
}
.section{
  width:100%;
  height:auto;
  background: #fff;
  padding-left:30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  border-top:1rpx solid #eee;
  }
.message-box{
  width:100%;
  height:127rpx;
  border-bottom:1rpx solid #eee;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.bottom-box{
  height:120rpx;
  position: fixed;
  bottom: 0rpx;
  background: #fff;
  width:750rpx;   
}
.bottom{
  height:88rpx;
  position: fixed;
  bottom: 30rpx;
  z-index: 999;
  width:690rpx;
  background:#F78210;
  line-height: 88rpx;
  text-align: center;
  font-size:32rpx;
  font-family:PingFang-SC-Regular;
  color:#fff;
  left:30rpx;
  border-radius: 10rpx;
}

