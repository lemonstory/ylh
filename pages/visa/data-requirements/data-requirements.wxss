/* pages/visa/data-requirements/data-requirements.wxss */
.handle-way {
     width: 100%;
     height: 90rpx;
     display: flex;
     justify-content: space-between;
     padding-left: 30rpx;
     padding-right: 30rpx;
     box-sizing: border-box;
     align-items: center;
     font-size: 32rpx;
     color: rgba(51, 51, 51, 1);
     border-bottom: 1rpx solid #eee;
     border-top: 1rpx solid #eee;
     background: #fff;
}
.tishi {
     width: 690rpx;
     height: 81rpx;
     margin-top: 20rpx;
     margin-bottom: 10rpx;
     background: #f4deca;
     border-radius: 10rpx;
     font-size: 24rpx;
     font-family: PingFang-SC-Regular;
     color: #666;
     display: flex;
     align-items: center;
     padding: 0 30rpx;
     box-sizing: border-box;
}
.star {
     color: rgba(255, 102, 0, 1);
}

.section-box {
     width: 100%;
     height: auto;
     background: #fff;
}

.electronic-data {
     margin-top: 10rpx;
     width: 100%;
     height: auto;
     background: #fff;
     padding-bottom: 10rpx;
    
}

.electronic-title {
     width: 690rpx;
     margin-left: 30rpx;
     font-size: 30rpx;
     border-bottom: 1rpx solid #eee;
     color: rgba(51, 51, 51, 1);
     height: auto;
     line-height: 76rpx;
}

.electronic-detail {
     display: flex;
     width: 630rpx;
     font-size: 26rpx;
     color: #333;
     align-items: center;
     height: auto;
     line-height: 80rpx;
     justify-content: space-between;
     
    
}

.electronic-detail-info {
     font-size: 26rpx;
     color: rgba(153, 153, 153, 1);
     line-height: 48rpx;
     display: flex;
     flex-direction: column;
     padding-bottom: 20rpx;
}
.data-example{
     width: 100rpx;
     height:50rpx;
     display: flex;
     border-radius: 10rpx;
     border:1rpx solid #eee;
     align-items: center;
     justify-content: center;
     color: rgba(153, 153, 153, 1);
}
.bottom {
     width: 750rpx;
     height: 98rpx;
     background: rgba(255, 255, 255, 1);
     position: fixed;
     bottom: 0;
}
.yuding {
     width: 690rpx;
     height: 88rpx;
     background: rgba(247, 130, 16, 1);
     border-radius: 10rpx;
     position: absolute;
     top: 5rpx;
     right: 30rpx;
     font-size: 32rpx;
     font-family: PingFang-SC-Regular;
     color: rgba(255, 255, 255, 1);
     text-align: center;
     line-height: 88rpx;
}

