/* pages/visa/marital-status/marital-status.wxss */
.idcard-box {
     width: 100%;
     box-sizing: border-box;
     overflow: hidden;
     height: 85rpx;
     padding: 0 30rpx;
     display: flex;
     justify-content: space-between;
     border-top: 1rpx solid #eee;
     background: #fff;
     font-size: 26rpx;
     color: rgba(51, 51, 51, 1);
     align-items: center;
     border-bottom:1rpx solid #eee;
}
.section{
     width: 100%;
     height: auto;
     padding: 30rpx;
     box-sizing: border-box;
     background: #fff;
     margin-top: 10rpx;
}
.id-box{
     width: 100%;
     height: auto;
     display: flex;
     justify-content: space-between;
}
.idcard{
     width: 335rpx;
     height:435rpx;
     border-radius: 10rpx;
}
.idcard image{
     width: 100%;
     height: 100%;
}
.camera-box {
     width: 100%;
     height: auto;
     display: flex;
     justify-content: space-between;
     margin-bottom: 30rpx;
}
.camera{
     width: 335rpx;
     height: 223rpx;
     border-radius: 10rpx;
}
.camera image{
     width: 100%;
     height: 100%;
}
.bottom{
     width: 690rpx;
     height: 88rpx;
     background: #f78210;
     border-radius: 10rpx;
     position: fixed;
     bottom: 30rpx;
     left: 30rpx;
     font-size: 32rpx;
     font-family: PingFang-SC-Regular;
     color: rgba(255, 255, 255, 1);
     text-align: center;
     line-height: 88rpx;
}
.ischeck{
     width: 24rpx;
     height:24rpx;
}

.ischeck image{
     width:100%;
     height:100%;
}

.idcard-hidden{
     width:690rpx;
     height:auto;
     background: #fff;
     box-sizing: border-box;
    
}
.idcard-info{
     width:100%;
     display:flex;
     align-items:center;
     height:84rpx;
     border-bottom: 1rpx solid #eee;
     font-size:26rpx;
     color:rgba(51,51,51,1);
     justify-content: space-around;
}

